{
    "name": "unionteams/laravel-unitpay",
    "description": "UnitPay payments for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "UnionTeams",
            "email": "unionteams@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "UnionTeams\\UnitPay\\Facades\\": "src/Facades",
